}

class Functor f {
  fn fmap(fn (a) collision, f a) f collision
}

data Either c d {
  Left(c)
  Right(d)
}

instance Functor (Either a) {
  fn fmap(f, either) => match either {
    Right(x) => Right(f(x))
    Left(y)  => Left(y)
  }
}


