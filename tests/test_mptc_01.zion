# test: pass, noprelude

data List a {
    Cons(a, List a)
    Nil
}

class Pushable c e {
    fn push(c, e) c
}

instance Pushable (List a) a {
    fn push(l, v) => match l {
        Cons(hd, tl) => Cons(hd, push(tl, v))
        Nil => Cons(v, Nil)
    }
}

fn main() {
    h := Nil
    g := push(h, 2)
    static_print(g)
    __builtin_pass_test
}

